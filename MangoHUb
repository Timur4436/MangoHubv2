local function createGui(parent)
	--ScreenGui1--
	local ScreenGui1 = Instance.new("ScreenGui",parent)
	ScreenGui1["ZIndexBehavior"] = Enum.ZIndexBehavior.Sibling
	----ScreenGui1----
	--Frame2--
	local Frame2 = Instance.new("Frame",ScreenGui1)
	Frame2["Name"] = "teleport6"
	Frame2["Position"] = UDim2.new(0.170952052, 0, 0.781173587, 0)
	Frame2["BorderColor3"] = Color3.new(0, 0, 0)
	Frame2["Size"] = UDim2.new(0.216122314, 0, 0.0721271411, 0)
	Frame2["BorderSizePixel"] = 0
	Frame2["BackgroundColor3"] = Color3.new(0.952941, 1, 0.027451)
	----Frame2----
	--Script3--
	local Script3 = Instance.new("Script",Frame2)
	Script3["Name"] = "Dragify"
	----Script3----
	--UICorner4--
	local UICorner4 = Instance.new("UICorner",Frame2)
	----UICorner4----
	--UIStroke5--
	local UIStroke5 = Instance.new("UIStroke",Frame2)
	UIStroke5["Color"] = Color3.new(1, 0.572549, 0.0470588)
	UIStroke5["Thickness"] = 5
	----UIStroke5----
	--TextButton6--
	local TextButton6 = Instance.new("TextButton",Frame2)
	TextButton6["TextWrapped"] = true
	TextButton6["TextColor3"] = Color3.new(0, 0, 0)
	TextButton6["BorderColor3"] = Color3.new(0, 0, 0)
	TextButton6["Text"] = "Mango steal up"
	TextButton6["Font"] = Enum.Font.Roboto
	TextButton6["TextSize"] = 14
	TextButton6["Position"] = UDim2.new(0.00750462571, 0, 0.179349214, 0)
	TextButton6["TextScaled"] = true
	TextButton6["Size"] = UDim2.new(0.984450281, 0, 0.630687416, 0)
	TextButton6["BorderSizePixel"] = 0
	TextButton6["BackgroundColor3"] = Color3.new(1, 1, 0.00784314)
	----TextButton6----
	--Script7--
	local Script7 = Instance.new("Script",TextButton6)
	----Script7----
	--Script8--
	local Script8 = Instance.new("Script",TextButton6)
	----Script8----
	--Script9--
	local Script9 = Instance.new("Script",ScreenGui1)
	Script9["Name"] = "Gui2Lua-teleport6"
	----Script9----
	spawn(function() --Source for Script3
		local script = Script3
		local UserInputService = game:GetService"UserInputService"
		
		function Dragify (Object)
			local Button1Down
		
			Object.InputBegan:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					Button1Down = true
		
					local A = UserInputService:GetMouseLocation()
		
					local Difference = Object.AbsolutePosition - A
		
					repeat task.wait()
						local B = UserInputService:GetMouseLocation()
						local Position = UDim2.fromOffset(
							B.X + Difference.X + Object.AnchorPoint.X * Object.AbsoluteSize.X, 
							B.Y + Difference.Y + Object.AnchorPoint.Y * Object.AbsoluteSize.Y
						)
		
						Object.Position = Position
					until not Button1Down
				end
			end)
		
			Object.InputEnded:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					Button1Down = false
				end
			end)
		end
		
		Dragify(script.Parent)
	end)
	spawn(function() --Source for Script7
		local script = Script7
		local button = script.Parent
		
		button.MouseButton1Click:Connect(function()
			local player = game.Players.LocalPlayer
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
		
			-- Находим что-то над головой игрока
			local rayOrigin = humanoidRootPart.Position
			local rayDirection = Vector3.new(0, 100, 0) -- Луч вверх на 100 studs
			local raycastParams = RaycastParams.new()
			raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
			raycastParams.FilterDescendantsInstances = {character}
		
			local raycastResult = workspace:Raycast(rayOrigin, rayDirection, raycastParams)
		
			if raycastResult then
				-- Если нашли препятствие, телепортируемся под ним
				local newPosition = raycastResult.Position - Vector3.new(0, 8, 0)
				humanoidRootPart.CFrame = CFrame.new(newPosition)
			else
				-- Если ничего нет, просто телепортируемся выше
				humanoidRootPart.CFrame = humanoidRootPart.CFrame + Vector3.new(0, 50, 0)
			end
		
			-- Уведомление
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "MangoHub",
				Text = "Teleport up",
				Duration = 5
			})
		end)
		
	end)
	spawn(function() --Source for Script8
		local script = Script8
		local Player = game:GetService("Players").LocalPlayer
		
		function SafeTeleportUp()
			local Character = Player.Character
			if not Character then return end
		
			local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
			if not HumanoidRootPart then return end
		
			-- Античит метод для Still Brain Road
			local originalCF = HumanoidRootPart.CFrame
			local newPosition = originalCF.Position + Vector3.new(0, 50, 0)
		
			-- Плавное перемещение вместо мгновенной телепортации
			for i = 1, 20 do
				HumanoidRootPart.CFrame = originalCF + Vector3.new(0, i * 2.5, 0)
				wait(0.01)
			end
		
			-- Фиксируем позицию
			HumanoidRootPart.CFrame = CFrame.new(newPosition)
		
			-- Дополнительная защита от античита
			wait(0.1)
			if Character:FindFirstChild("Humanoid") then
				Character.Humanoid:ChangeState(Enum.HumanoidStateType.Landed)
			end
		end
		
		-- Вставляй эту функцию в свою кнопку
		SafeTeleportUp()
		
	end)
	spawn(function() --Source for Script9
		local script = Script9
		local function createGui(parent)
			--Frame1--
			local Frame1 = Instance.new("Frame",parent)
			Frame1["Name"] = "teleport6"
			Frame1["Position"] = UDim2.new(0.170952052, 0, 0.781173587, 0)
			Frame1["BorderColor3"] = Color3.new(0, 0, 0)
			Frame1["Size"] = UDim2.new(0.216122314, 0, 0.0721271411, 0)
			Frame1["BorderSizePixel"] = 0
			Frame1["BackgroundColor3"] = Color3.new(0.952941, 1, 0.027451)
			----Frame1----
			--Script2--
			local Script2 = Instance.new("Script",Frame1)
			Script2["Name"] = "Dragify"
			----Script2----
			--UICorner3--
			local UICorner3 = Instance.new("UICorner",Frame1)
			----UICorner3----
			--UIStroke4--
			local UIStroke4 = Instance.new("UIStroke",Frame1)
			UIStroke4["Color"] = Color3.new(1, 0.572549, 0.0470588)
			UIStroke4["Thickness"] = 5
			----UIStroke4----
			--TextButton5--
			local TextButton5 = Instance.new("TextButton",Frame1)
			TextButton5["TextWrapped"] = true
			TextButton5["TextColor3"] = Color3.new(0, 0, 0)
			TextButton5["BorderColor3"] = Color3.new(0, 0, 0)
			TextButton5["Text"] = "Mango steal up"
			TextButton5["Font"] = Enum.Font.Roboto
			TextButton5["TextSize"] = 14
			TextButton5["Position"] = UDim2.new(0.00750462571, 0, 0.179349214, 0)
			TextButton5["TextScaled"] = true
			TextButton5["Size"] = UDim2.new(0.984450281, 0, 0.630687416, 0)
			TextButton5["BorderSizePixel"] = 0
			TextButton5["BackgroundColor3"] = Color3.new(1, 1, 0.00784314)
			----TextButton5----
			--Script6--
			local Script6 = Instance.new("Script",TextButton5)
			----Script6----
			--Script7--
			local Script7 = Instance.new("Script",TextButton5)
			----Script7----
			spawn(function() --Source for Script2
				local script = Script2
				local UserInputService = game:GetService"UserInputService"
				
				function Dragify (Object)
					local Button1Down
				
					Object.InputBegan:Connect(function(Input)
						if Input.UserInputType == Enum.UserInputType.MouseButton1 then
							Button1Down = true
				
							local A = UserInputService:GetMouseLocation()
				
							local Difference = Object.AbsolutePosition - A
				
							repeat task.wait()
								local B = UserInputService:GetMouseLocation()
								local Position = UDim2.fromOffset(
									B.X + Difference.X + Object.AnchorPoint.X * Object.AbsoluteSize.X, 
									B.Y + Difference.Y + Object.AnchorPoint.Y * Object.AbsoluteSize.Y
								)
				
								Object.Position = Position
							until not Button1Down
						end
					end)
				
					Object.InputEnded:Connect(function(Input)
						if Input.UserInputType == Enum.UserInputType.MouseButton1 then
							Button1Down = false
						end
					end)
				end
				
				Dragify(script.Parent)
			end)
			spawn(function() --Source for Script6
				local script = Script6
				local button = script.Parent
				
				button.MouseButton1Click:Connect(function()
					local player = game.Players.LocalPlayer
					local character = player.Character or player.CharacterAdded:Wait()
					local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
				
					-- Находим что-то над головой игрока
					local rayOrigin = humanoidRootPart.Position
					local rayDirection = Vector3.new(0, 100, 0) -- Луч вверх на 100 studs
					local raycastParams = RaycastParams.new()
					raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
					raycastParams.FilterDescendantsInstances = {character}
				
					local raycastResult = workspace:Raycast(rayOrigin, rayDirection, raycastParams)
				
					if raycastResult then
						-- Если нашли препятствие, телепортируемся под ним
						local newPosition = raycastResult.Position - Vector3.new(0, 8, 0)
						humanoidRootPart.CFrame = CFrame.new(newPosition)
					else
						-- Если ничего нет, просто телепортируемся выше
						humanoidRootPart.CFrame = humanoidRootPart.CFrame + Vector3.new(0, 50, 0)
					end
				
					-- Уведомление
					game:GetService("StarterGui"):SetCore("SendNotification", {
						Title = "MangoHub",
						Text = "Teleport up",
						Duration = 5
					})
				end)
				
			end)
			spawn(function() --Source for Script7
				local script = Script7
				local Player = game:GetService("Players").LocalPlayer
				
				function SafeTeleportUp()
					local Character = Player.Character
					if not Character then return end
				
					local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
					if not HumanoidRootPart then return end
				
					-- Античит метод для Still Brain Road
					local originalCF = HumanoidRootPart.CFrame
					local newPosition = originalCF.Position + Vector3.new(0, 50, 0)
				
					-- Плавное перемещение вместо мгновенной телепортации
					for i = 1, 20 do
						HumanoidRootPart.CFrame = originalCF + Vector3.new(0, i * 2.5, 0)
						wait(0.01)
					end
				
					-- Фиксируем позицию
					HumanoidRootPart.CFrame = CFrame.new(newPosition)
				
					-- Дополнительная защита от античита
					wait(0.1)
					if Character:FindFirstChild("Humanoid") then
						Character.Humanoid:ChangeState(Enum.HumanoidStateType.Landed)
					end
				end
				
				-- Вставляй эту функцию в свою кнопку
				SafeTeleportUp()
				
			end)
		end
		createGui(game.CoreGui) --coregui by default you should but change it
	end)
end
createGui(game.CoreGui) --coregui by default you should but change it
