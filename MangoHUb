local function createGui(parent)
	--ScreenGui1--
	local ScreenGui1 = Instance.new("ScreenGui",parent)
	ScreenGui1["ZIndexBehavior"] = Enum.ZIndexBehavior.Sibling
	----ScreenGui1----
	--Frame2--
	local Frame2 = Instance.new("Frame",ScreenGui1)
	Frame2["Name"] = "teleport6"
	Frame2["Position"] = UDim2.new(0.618167877, 0, 0.816625893, 0)
	Frame2["BorderColor3"] = Color3.new(0, 0, 0)
	Frame2["Size"] = UDim2.new(0.216122314, 0, 0.0721271411, 0)
	Frame2["BorderSizePixel"] = 0
	Frame2["BackgroundColor3"] = Color3.new(0.952941, 1, 0.027451)
	----Frame2----
	--Script3--
	local Script3 = Instance.new("Script",Frame2)
	Script3["Name"] = "Dragify"
	----Script3----
	--UICorner4--
	local UICorner4 = Instance.new("UICorner",Frame2)
	----UICorner4----
	--UIStroke5--
	local UIStroke5 = Instance.new("UIStroke",Frame2)
	UIStroke5["Color"] = Color3.new(1, 0.572549, 0.0470588)
	UIStroke5["Thickness"] = 5
	----UIStroke5----
	--TextButton6--
	local TextButton6 = Instance.new("TextButton",Frame2)
	TextButton6["TextWrapped"] = true
	TextButton6["TextColor3"] = Color3.new(0, 0, 0)
	TextButton6["BorderColor3"] = Color3.new(0, 0, 0)
	TextButton6["Text"] = "Mango Instant Steal"
	TextButton6["Font"] = Enum.Font.Roboto
	TextButton6["TextSize"] = 14
	TextButton6["Position"] = UDim2.new(0.00750462571, 0, 0.179349214, 0)
	TextButton6["TextScaled"] = true
	TextButton6["Size"] = UDim2.new(0.984450281, 0, 0.630687416, 0)
	TextButton6["BorderSizePixel"] = 0
	TextButton6["BackgroundColor3"] = Color3.new(1, 1, 0.00784314)
	----TextButton6----
	--Script7--
	local Script7 = Instance.new("Script",TextButton6)
	----Script7----
	spawn(function() --Source for Script3
		local script = Script3
		local UserInputService = game:GetService"UserInputService"
		
		function Dragify (Object)
			local Button1Down
		
			Object.InputBegan:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					Button1Down = true
		
					local A = UserInputService:GetMouseLocation()
		
					local Difference = Object.AbsolutePosition - A
		
					repeat task.wait()
						local B = UserInputService:GetMouseLocation()
						local Position = UDim2.fromOffset(
							B.X + Difference.X + Object.AnchorPoint.X * Object.AbsoluteSize.X, 
							B.Y + Difference.Y + Object.AnchorPoint.Y * Object.AbsoluteSize.Y
						)
		
						Object.Position = Position
					until not Button1Down
				end
			end)
		
			Object.InputEnded:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					Button1Down = false
				end
			end)
		end
		
		Dragify(script.Parent)
	end)
	spawn(function() --Source for Script7
		local script = Script7
		local button = script.Parent
		local Player = game:GetService("Players").LocalPlayer
		local Character = Player.Character or Player.CharacterAdded:Wait()
		local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
		
		local savedPosition = nil
		local isTeleporting = false
		local originalPosition = nil
		
		-- –ó–∞–ø–æ–º–∏–Ω–∞–µ–º –ø–æ–∑–∏—Ü–∏—é –ø—Ä–∏ —Å–ø–∞–≤–Ω–µ (—Ç–≤–æ—é —É–∫—Ä–∞–¥–µ–Ω–Ω—É—é –±–∞–∑—É)
		local function saveStolenBasePosition()
			savedPosition = HumanoidRootPart.Position
			print("üìç –£–∫—Ä–∞–¥–µ–Ω–Ω–∞—è –±–∞–∑–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∞: " .. tostring(savedPosition))
		end
		
		-- –û–±—Ö–æ–¥ –∞–Ω—Ç–∏—á–∏—Ç–∞ Still Brainrot
		local function bypassAntiCheatTeleport(targetPosition)
			if isTeleporting then return end
			isTeleporting = true
		
			-- –°–æ—Ö—Ä–∞–Ω—è–µ–º —Ç–µ–∫—É—â—É—é –ø–æ–∑–∏—Ü–∏—é
			originalPosition = HumanoidRootPart.Position
		
			-- –ú–µ—Ç–æ–¥ 1: –ú–≥–Ω–æ–≤–µ–Ω–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –º–∞–ª—ã–º–∏ —à–∞–≥–∞–º–∏ (–æ–±—Ö–æ–¥ –∞–Ω—Ç–∏—á–∏—Ç–∞)
			local function gradualTeleport()
				local steps = 10
				local stepVector = (targetPosition - originalPosition) / steps
		
				for i = 1, steps do
					if not Character or not HumanoidRootPart then break end
		
					-- –ú–∞–ª—ã–º–∏ —à–∞–≥–∞–º–∏ –¥–≤–∏–≥–∞–µ–º—Å—è –∫ —Ü–µ–ª–∏
					local nextPosition = originalPosition + (stepVector * i)
					HumanoidRootPart.CFrame = CFrame.new(nextPosition)
					wait(0.05) -- –ö–æ—Ä–æ—Ç–∫–∞—è –ø–∞—É–∑–∞ –º–µ–∂–¥—É —à–∞–≥–∞–º–∏
				end
			end
		
			-- –ú–µ—Ç–æ–¥ 2: –¢–µ–ª–µ–ø–æ—Ä—Ç —á–µ—Ä–µ–∑ CFrame + –Ω–µ–±–æ–ª—å—à–æ–π —Ä–∞–Ω–¥–æ–º
			local function cframeTeleport()
				local randomOffset = Vector3.new(
					math.random(-2, 2),
					math.random(0, 3),
					math.random(-2, 2)
				)
				HumanoidRootPart.CFrame = CFrame.new(targetPosition + randomOffset)
			end
		
			-- –ü—ã—Ç–∞–µ–º—Å—è –æ–±–æ–π—Ç–∏ –∞–Ω—Ç–∏—á–∏—Ç
			print("üõ°Ô∏è –û–±—Ö–æ–¥ –∞–Ω—Ç–∏—á–∏—Ç–∞ Still Brainrot...")
		
			-- –ü—Ä–æ–±—É–µ–º —Ä–∞–∑–Ω—ã–µ –º–µ—Ç–æ–¥—ã
			gradualTeleport()
			wait(0.1)
			cframeTeleport()
		
			print("‚úÖ –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω –Ω–∞ —É–∫—Ä–∞–¥–µ–Ω–Ω—É—é –±–∞–∑—É!")
		
			-- –ñ–¥–µ–º 5 —Å–µ–∫—É–Ω–¥ –Ω–∞ –±–∞–∑–µ
			wait(5)
		
			-- –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –æ–±—Ä–∞—Ç–Ω–æ (—Ç–æ–∂–µ —Å –æ–±—Ö–æ–¥–æ–º –∞–Ω—Ç–∏—á–∏—Ç–∞)
			print("‚Ü©Ô∏è –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –æ–±—Ä–∞—Ç–Ω–æ...")
			gradualTeleport()
			wait(0.1)
			cframeTeleport()
		
			isTeleporting = false
			print("üîô –í–æ–∑–≤—Ä–∞—Ç –∑–∞–≤–µ—Ä—à–µ–Ω!")
		end
		
		-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–Ω–æ–ø–∫–∏
		button.MouseButton1Click:Connect(function()
			if savedPosition and not isTeleporting then
				-- –í–∏–∑—É–∞–ª—å–Ω–æ–µ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
				game:GetService("StarterGui"):SetCore("SendNotification", {
					Title = "–¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è",
					Text = "–û–±—Ö–æ–¥ –∞–Ω—Ç–∏—á–∏—Ç–∞...",
					Duration = 3
				})
		
				bypassAntiCheatTeleport(savedPosition)
		
			else
				warn("–ë–∞–∑–∞ –Ω–µ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∞ –∏–ª–∏ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è —É–∂–µ –∏–¥–µ—Ç!")
			end
		end)
		
		-- –ê–≤—Ç–æ-—Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –ø–æ–∑–∏—Ü–∏–∏ –ø—Ä–∏ –∑–∞—Ö–æ–¥–µ –≤ –∏–≥—Ä—É
		wait(3) -- –ñ–¥–µ–º –∑–∞–≥—Ä—É–∑–∫–∏
		if HumanoidRootPart then
			saveStolenBasePosition()
		end
		
		-- –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –ø–æ–∑–∏—Ü–∏–∏ –ø—Ä–∏ —Å–º–µ–Ω–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
		Player.CharacterAdded:Connect(function(character)
			Character = character
			HumanoidRootPart = character:WaitForChild("HumanoidRootPart")
			wait(2)
			saveStolenBasePosition()
		end)
		
		-- –ê–Ω—Ç–∏-–∞—Ñ–∫ —Å–∏—Å—Ç–µ–º–∞
		local VirtualUser = game:GetService('VirtualUser')
		game:GetService('Players').LocalPlayer.Idled:connect(function()
			VirtualUser:CaptureController()
			VirtualUser:ClickButton2(Vector2.new())
		end)
		
		-- –ê–≤—Ç–æ-–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–æ–∑–∏—Ü–∏–∏ –±–∞–∑—ã –∫–∞–∂–¥—ã–µ 30 —Å–µ–∫—É–Ω–¥
		while true do
			wait(30)
			if HumanoidRootPart and not isTeleporting then
				saveStolenBasePosition()
				print("üîÑ –ü–æ–∑–∏—Ü–∏—è –±–∞–∑—ã –æ–±–Ω–æ–≤–ª–µ–Ω–∞!")
			end
		end
		
	end)
end
createGui(game.CoreGui) --coregui by default you should but change it
