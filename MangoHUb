	UICorner17["CornerRadius"] = UDim.new(0, 18)
	----UICorner17----
	--TextButton18--
	local TextButton18 = Instance.new("TextButton",Frame15)
	TextButton18["TextWrapped"] = true
	TextButton18["TextColor3"] = Color3.new(0, 0, 0)
	TextButton18["BorderColor3"] = Color3.new(0, 0, 0)
	TextButton18["Text"] = "Instant Steal  (Off)"
