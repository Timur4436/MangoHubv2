	spawn(function() --Source for Script3
		local script = Script3
		local UserInputService = game:GetService"UserInputService"
		
		function Dragify (Object)
			local Button1Down
		
			Object.InputBegan:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					Button1Down = true
		
					local A = UserInputService:GetMouseLocation()
		
					local Difference = Object.AbsolutePosition - A
		
					repeat task.wait()
						local B = UserInputService:GetMouseLocation()
						local Position = UDim2.fromOffset(
							B.X + Difference.X + Object.AnchorPoint.X * Object.AbsoluteSize.X, 
							B.Y + Difference.Y + Object.AnchorPoint.Y * Object.AbsoluteSize.Y
						)
		
						Object.Position = Position
					until not Button1Down
				end
			end)
		
			Object.InputEnded:Connect(function(Input)
				if Input.UserInputType == Enum.UserInputType.MouseButton1 then
					Button1Down = false
				end
			end)
		end
		
		Dragify(script.Parent)
	end)
	spawn(function() --Source for Script6
		local script = Script6
		local button = script.Parent
		
		-- Запоминаем исходные значения
		local originalColor = button.BackgroundColor3
		local originalText = button.Text
		
		-- Значения при активации
		local activeColor = Color3.fromRGB(0, 255, 0) -- Зелёный
		local activeText = "Instant Steal (On)"
		
		-- Переменная-флаг
		local isActive = false
		
		button.MouseButton1Click:Connect(function()
			-- Меняем состояние
			isActive = not isActive
		
			if isActive then
				-- Активируем: зелёный цвет и новый текст
				button.BackgroundColor3 = activeColor
				button.Text = activeText
			else
				-- Деактивируем: исходный цвет и исходный текст
				button.BackgroundColor3 = originalColor
				button.Text = originalText
			end
		end)
		
	end)
	spawn(function() --Source for Script7
		local script = Script7
		local button = script.Parent
		local label = script.Parent.Parent:FindFirstChild("Label")
		
		if not label then
			warn("Label не найден!")
			return
		end
		
		-- Запоминаем исходный текст
		local originalText = label.Text
		-- Состояние: false = исходный текст, true = изменённый текст
		local isChanged = false
		
		button.MouseButton1Click:Connect(function()
			-- Меняем состояние на противоположное
			isChanged = not isChanged
		
			if isChanged then
				-- Меняем текст на новый
				label.Text = "Stealing..."
			else
				-- Возвращаем исходный текст
				label.Text = originalText
			end
		end)
		
	end)
	spawn(function() --Source for Script8
		local script = Script8
		local button = script.Parent
		local Player = game:GetService("Players").LocalPlayer
		
		local savedPosition = nil
		
		-- Функция для мгновенного телепорта
		local function teleportToPosition(position)
			pcall(function()
				local character = Player.Character
				if character and character:FindFirstChild("HumanoidRootPart") then
					character.HumanoidRootPart.CFrame = CFrame.new(position)
				end
			end)
		end
		
		-- Запоминаем позицию при запуске
		local function savePosition()
			local character = Player.Character
			if character and character:FindFirstChild("HumanoidRootPart") then
				savedPosition = character.HumanoidRootPart.Position
				print("Позиция сохранена: " .. tostring(savedPosition))
			end
		end
		
		-- Обработчик нажатия кнопки
		button.MouseButton1Click:Connect(function()
			if savedPosition then
				teleportToPosition(savedPosition)
				print("Телепортирован на базу!")
			else
				warn("Позиция не сохранена!")
			end
		end)
		
		-- Автоматически сохраняем позицию при появлении
		Player.CharacterAdded:Connect(function(character)
			character:WaitForChild("HumanoidRootPart")
			wait(1)
			savePosition()
		end)
		
		-- Сохраняем при старте если персонаж уже есть
		if Player.Character then
			wait(1)
			savePosition()
		end
		
	end)
	spawn(function() --Source for Script11
		local script = Script11
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		
		local button = script.Parent -- Это ваша TextButton
		
		-- Ждем когда загрузится PlayerGui
		player:WaitForChild("PlayerGui")
		
		-- Функция создания черного экрана
		local function createBlackScreen()
			-- Создаем ScreenGui если его нет
			local screenGui = player.PlayerGui:FindFirstChild("BlackScreenGui")
			if not screenGui then
				screenGui = Instance.new("ScreenGui")
				screenGui.Name = "BlackScreenGui"
				screenGui.Parent = player.PlayerGui
			end
		
			-- Очищаем предыдущий экран
			screenGui:ClearAllChildren()
		
			-- Создаем черный экран
			local blackScreen = Instance.new("Frame")
			blackScreen.Name = "BlackScreen"
			blackScreen.Size = UDim2.new(1, 0, 1, 0)
			blackScreen.Position = UDim2.new(0, 0, 0, 0)
			blackScreen.BackgroundColor3 = Color3.new(0, 0, 0)
			blackScreen.BackgroundTransparency = 0
			blackScreen.BorderSizePixel = 0
			blackScreen.ZIndex = 100
			blackScreen.Parent = screenGui
		
			-- Текст посередине
			local textLabel = Instance.new("TextLabel")
			textLabel.Name = "TeleportText"
			textLabel.Size = UDim2.new(0, 300, 0, 50)
			textLabel.Position = UDim2.new(0.5, -150, 0.5, -25)
			textLabel.AnchorPoint = Vector2.new(0.5, 0.5)
			textLabel.BackgroundTransparency = 1
			textLabel.Text = "Mango hub is teleporting you..."
			textLabel.TextColor3 = Color3.new(1, 1, 1)
			textLabel.TextSize = 20
			textLabel.Font = Enum.Font.GothamBold
			textLabel.ZIndex = 101
			textLabel.Parent = blackScreen
		
			-- Анимация многоточия
			local dots = ""
			local dotCount = 0
		
			local function updateDots()
				dotCount = (dotCount % 3) + 1
				dots = string.rep(".", dotCount)
				textLabel.Text = "Mango hub is teleporting you" .. dots
			end
		
			-- Запускаем анимацию
			local heartbeatConnection
			heartbeatConnection = game:GetService("RunService").Heartbeat:Connect(updateDots)
		
			-- Удаляем через 3 секунды
			task.delay(3, function()
				if heartbeatConnection then
					heartbeatConnection:Disconnect()
				end
				if screenGui and screenGui.Parent then
					screenGui:Destroy()
				end
			end)
		end
		
		-- Подключаем нажатие кнопки
		button.MouseButton1Click:Connect(function()
			createBlackScreen()
		end)
		
		print("Скрипт активирован! Нажмите на кнопку для черного экрана")
		
	end)
