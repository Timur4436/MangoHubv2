				label.Text = "Stealing..."
			else
				-- Возвращаем исходный текст
				label.Text = originalText
			end
		end)
		
	end)
	spawn(function() --Source for Script23
		local script = Script23
		local ReplicatedStorage = game:GetService("ReplicatedStorage")
		local Players = game:GetService("Players")
		
		local player = Players.LocalPlayer
		local TeleportEvent = ReplicatedStorage:WaitForChild("SafeTeleportEvent")
		
		-- Обработчик нажатия кнопки [citation:5]
		script.Parent.MouseButton1Click:Connect(function()
			TeleportEvent:FireServer()
		end)
		
	end)
end
createGui(game.CoreGui) --coregui by default you should but change it
