	TextLabel70["Position"] = UDim2.new(0.126394048, 0, -0.0438596494, 0)
	TextLabel70["TextScaled"] = true
	TextLabel70["Size"] = UDim2.new(0.743494451, 0, 0.438596487, 0)
	TextLabel70["BorderSizePixel"] = 0
	TextLabel70["BackgroundColor3"] = Color3.new(1, 1, 1)
	----TextLabel70----
	--UITextSizeConstraint71--
	local UITextSizeConstraint71 = Instance.new("UITextSizeConstraint",TextLabel70)
	UITextSizeConstraint71["MaxTextSize"] = 29
